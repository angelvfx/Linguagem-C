#include <stdio.h>

int main(){
	
	int notas[5];
	int No = 0;
	float media;
	int n = 1;
	
	for(No = 0; No < 5; No++){
		printf("\nDigite a nota da prova %d: ", n ++);
		scanf("%d", &notas[No]);	}		
	
	
	for(No = 0; No < 5; No++){
		media += notas[No];}
	
	printf("\nA nota do aluno foi: %.2f", media/5);
	
}
